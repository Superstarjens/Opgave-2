#include <stdio.h>

int main(void)
{
    int seconds, minutes, hours;

    printf("Enter seconds: ");
    scanf("%d", &seconds);

    minutes=seconds/60;
    seconds=seconds%60;
    hours=minutes/60;
    secounds=minutes%60;
    
    printf(¨%d secound, %d minutes, %d hours \n¨, secounds, minutes, hours);
    return(0);
}
